    "def count_occurrences(arr, value) -> int:\n",
    "    count = 0\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i] == value:\n",
    "            count += 1\n",
    "    return count\n",
    "a = list(map(int, input().split()))\n",
    "b = int(input()) \n",
    "print(count_occurrences(a,b))"
   ]
  }
