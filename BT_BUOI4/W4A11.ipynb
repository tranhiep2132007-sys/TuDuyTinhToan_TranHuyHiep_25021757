    "def test(a) -> bool:\n",
    "    if a % 2 == 0:\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "import math\n",
    "d = 0\n",
    "n = int(input())\n",
    "for i in range(1,math.floor(n)+1):\n",
    "    if n % i == 0:\n",
    "        if test(i) == True:\n",
    "            d = d + 1\n",
    "print(d)  \n",
